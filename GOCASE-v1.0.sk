#Komenda dla graczy bez permisji#
command /caselist:
	trigger:
		open chest with 6 rows named "&6&l** &2&lLISTA SKRZYNEK" to player
		set {_num} to 0
		loop {GOCASE::cases::*}:			
			wait 2 ticks
			format slot {_num} of player with 1 of chest named "&5SKRZYNKA %loop-value%" to close then run "caseilist %player% %loop-value%"
			add 1 to {_num}
