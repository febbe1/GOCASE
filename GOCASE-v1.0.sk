						wait 10 seconds
						send "&6**&c&l5"
						wait 1 second
						send "&6**&c&l4"
						wait 1 second
						send "&6**&c&l3"
						wait 1 second
						send "&6**&c&l2"
						wait 1 second
						send "&6**&c&l1"
